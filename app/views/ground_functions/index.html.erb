<h1>Listing ground_functions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Symbol</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ground_functions.each do |ground_function| %>
  <tr>
    <td><%= ground_function.name %></td>
    <td><%= ground_function.symbol %></td>
    <td><%= ground_function.code %></td>
    <td><%= link_to 'Show', ground_function %></td>
    <td><%= link_to 'Edit', edit_ground_function_path(ground_function) %></td>
    <td><%= link_to 'Destroy', ground_function, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

