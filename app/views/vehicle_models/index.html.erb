<h1>Listing vehicle_models</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>Country</th>
    <th>Image</th>
    <th></th>
  </tr>

<% @vehicle_models.each do |vehicle_model| %>
  <tr>
    <td><%= vehicle_model.name %></td>
    <td><%= vehicle_model.vehicletype %></td>
    <td><%= vehicle_model.country %></td>
    <td><%= vehicle_model.image %></td>
    <td><%= link_to 'Show', vehicle_model %></td>
    <td><%= link_to 'Edit', edit_vehicle_model_path(vehicle_model) %></td>
    <td><%= link_to 'Destroy', vehicle_model, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vehicle model', new_vehicle_model_path %>
