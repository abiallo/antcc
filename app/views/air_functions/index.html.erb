<h1>Listing air_functions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Symbol</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @air_functions.each do |air_function| %>
  <tr>
    <td><%= air_function.name %></td>
    <td><%= air_function.symbol %></td>
    <td><%= air_function.code %></td>
    <td><%= link_to 'Show', air_function %></td>
    <td><%= link_to 'Edit', edit_air_function_path(air_function) %></td>
    <td><%= link_to 'Destroy', air_function, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Air function', new_air_function_path %>
