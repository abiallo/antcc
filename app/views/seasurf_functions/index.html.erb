<h1>Listing seasurf_functions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Symbol</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seasurf_functions.each do |seasurf_function| %>
  <tr>
    <td><%= seasurf_function.name %></td>
    <td><%= seasurf_function.symbol %></td>
    <td><%= seasurf_function.code %></td>
    <td><%= link_to 'Show', seasurf_function %></td>
    <td><%= link_to 'Edit', edit_seasurf_function_path(seasurf_function) %></td>
    <td><%= link_to 'Destroy', seasurf_function, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

