<h1>Listing seasub_functions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Symbol</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seasub_functions.each do |seasub_function| %>
  <tr>
    <td><%= seasub_function.name %></td>
    <td><%= seasub_function.symbol %></td>
    <td><%= seasub_function.code %></td>
    <td><%= link_to 'Show', seasub_function %></td>
    <td><%= link_to 'Edit', edit_seasub_function_path(seasub_function) %></td>
    <td><%= link_to 'Destroy', seasub_function, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

